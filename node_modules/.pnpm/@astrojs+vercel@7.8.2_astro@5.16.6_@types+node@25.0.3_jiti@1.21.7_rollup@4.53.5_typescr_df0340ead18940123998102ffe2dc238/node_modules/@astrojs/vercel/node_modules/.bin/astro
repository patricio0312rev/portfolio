#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/patricio/Sites/portfolio/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/Users/patricio/Sites/portfolio/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/patricio/Sites/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/patricio/Sites/portfolio/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules/astro/node_modules:/Users/patricio/Sites/portfolio/node_modules/.pnpm/astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules:/Users/patricio/Sites/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.16.6_@types+node@25.0.3_jiti@1.21.7_rollup@4.53.5_typescript@5.9.3_yaml@2.8.2/node_modules/astro/astro.js" "$@"
fi
